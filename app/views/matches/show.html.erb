<div class="container-fluid">
  <p id="notice">
    <%= notice %>
  </p>

  <div class="form-group mb-3">
    <p><strong>Title:</strong></p>
    <div>
      <%= @match.title %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>Description:</strong></p>
    <div>
      <%= @match.description %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>User:</strong></p>
    <div>
      <%= @match.user_id %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>Game:</strong></p>
    <div>
      <%= @match.game_id %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>Location:</strong></p>
    <div>
      <%= @match.location %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>Number of players:</strong></p>
    <div>
      <%= @match.number_of_players %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>Start at:</strong></p>
    <div>
      <%= @match.start_at %>
    </div>
  </div>

  <div class="form-group mb-3">
    <p><strong>End at:</strong></p>
    <div>
      <%= @match.end_at %>
    </div>
  </div>


  <%= link_to 'Edit' , edit_match_path(@match), class: "btn btn-primary min-width-btn" %>
    <%= link_to 'Back' , matches_path, class: "btn btn-light min-width-btn" %>
</div>